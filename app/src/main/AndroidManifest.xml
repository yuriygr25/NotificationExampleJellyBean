<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yura.notificationexamplejellybean">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity2"
            android:label="@string/about_title_activity2">
        </activity>
        <activity android:name=".Activity3"
            android:label="@string/about_title_activity3">
        </activity>
        <activity android:name=".Activity4"
            android:label="@string/about_title_activity4">
        </activity>
        <activity android:name=".Activity5"
            android:label="@string/about_title_activity5">
        </activity>

    </application>

</manifest>